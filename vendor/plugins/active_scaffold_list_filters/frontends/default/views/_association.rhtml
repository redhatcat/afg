<% filter.associated_values.each do |id, label| -%>
  <%
    session_value = nil
    session_value = (filter_session.include? id.to_s) unless filter_session.nil?
    session_value = (filter.params.include? id.to_s) unless filter.params.nil?
  -%>
      <div style='display: inline; float: left; width: 17em; overflow: hidden;'>
        <%= content_tag(:label,
          check_box_tag(list_filter_input_name(filter) + "[]",
            "#{id}",session_value) + "&nbsp;#{label}") %>
      </div>
<% end -%>
